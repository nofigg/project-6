{
  "extends": [
    "next/core-web-vitals",
    "plugin:react/recommended",
    "plugin:react-hooks/recommended",
    "plugin:@typescript-eslint/recommended",
    "prettier"
  ],
  "plugins": ["react-refresh"],
  "rules": {
    "react-refresh/only-export-components": ["error", { "allowConstantExport": true }]
  },
  "settings": {
    "react": {
      "version": "detect"
    }
  }
}
